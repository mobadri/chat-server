<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="208.0" prefWidth="505.0" stylesheets="@../../static/css/style.css"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.chat.server.view.server.controller.confirmation.ConfirmationController">
    <children>
        <Pane id="title-pane" prefHeight="64.0" prefWidth="505.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Label fx:id="messageTitleLabel" layoutX="14.0" layoutY="17.0" prefHeight="30.0" prefWidth="478.0"
                   text="Confirmation message" textFill="WHITE">
                <font>
                    <Font size="18.0"/>
                </font>
            </Label>
        </Pane>
        <Pane layoutY="64.0" prefHeight="82.0" prefWidth="505.0">
            <children>
                <Label fx:id="messageLabel" layoutX="12.0" layoutY="33.0" prefHeight="21.0" prefWidth="478.0"
                       text="Are you sure you wan't do delete this user ?">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
            </children>
        </Pane>
        <Pane layoutX="0" layoutY="165.0" prefHeight="50.0" prefWidth="505.0">
            <children>
                <Button fx:id="okButton" layoutX="103.0" layoutY="10.0" onAction="#onOkAction" prefHeight="30"
                        prefWidth="100" text="Ok"/>
                <Button fx:id="cancelButton" layoutX="302.0" layoutY="10.0" onAction="#onCancelAction" prefHeight="30"
                        prefWidth="100" text="Cancel"/>
            </children>
        </Pane>
    </children>
</AnchorPane>
